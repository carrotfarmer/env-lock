{
  "name": "env-lock",
  "version": "0.0.0",
  "description": "Save your .env files securely",
  "author": "Dhruva Srinivas @carrotfarmer",
  "bin": {
    "env-lock": "./bin/run"
  },
  "homepage": "https://github.com/carrotfarmer/env-lock",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "carrotfarmer/env-lock",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.3",
    "@types/better-sqlite3": "^7.6.3",
    "better-sqlite3": "^8.2.0",
    "chalk": "4.1.2",
    "cryptr": "^6.2.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.13",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.20",
    "chai": "^4",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "env-lock",
    "dirname": "env-lock",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/commands/root.tests.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/carrotfarmer/env-lock/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
